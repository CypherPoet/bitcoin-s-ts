{
  "name": "wallet-server-ui-proxy",
  "version": "1.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoin-s/bitcoin-s-ts.git",
    "directory": "wallet-server-ui-proxy"
  },
  "scripts": {
    "build": "node --loader ts-node/esm ./build/buildVersion.ts && cp config.json build.json ./dist",
    "start": "node --loader ts-node/esm --es-module-specifier-resolution=node server.ts",
    "startlocal": "DEFAULT_UI_PASSWORD=none BITCOIN_S_SERVER_RPC_PASSWORD=password node --loader ts-node/esm --es-module-specifier-resolution=node server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "dependencies": {
    "@bitcoin-s-ts/wallet-ts": "file:../wallet-ts",
    "common-ts": "file:../common-ts",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "socks-proxy-agent": "^6.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.7.6",
    "git-last-commit": "^1.0.1",
    "nodemon": "^2.0.12",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
  }
}
